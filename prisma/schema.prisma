model sys_Migration_log {
  id             Int      @id @default(autoincrement())
  from_tableName String
  to_tableName   String
  migratedAt     DateTime
  status         String
}

model sys_Role {
  id      String                 @id @db.Char(20)
  name    String                 @db.VarChar(20)
  iStatus MasterRecordStatusEnum @default(ACTIVE)
  remarks String?                @db.VarChar(255)
}

